{
  "name": "cangyun-monorepo",
  "private": true,
  "version": "0.1.0",
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "backend",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter ./backend run build",
    "dev": "pnpm -r --parallel --filter ./backend run start:dev",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "format": "pnpm -r format"
  }
}
